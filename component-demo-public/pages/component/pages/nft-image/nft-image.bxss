.overflow {
  width: 100%;
  overflow-x: scroll;
  overflow-y: hidden;
  white-space: nowrap;
  padding-left: 15px;
}
.overflow view {

}
.overflow-cover {
  position: relative;
  display: inline-block;
  overflow: hidden;
  border-radius: 8px;
  width: 162px;
  height: 37px;
  background: #fff;
  margin-right: 16px;
}
.overflow bn-image {
  width: 162px;
  height: 37px;
  background: #fff;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 2;
}
