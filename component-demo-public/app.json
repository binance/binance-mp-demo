{
  "darkmode": true,
  "entryPage": "pages/tabbar/component/index",
  "pages": [
    "pages/tabbar/component/index",
    "pages/tabbar/extended/index",
    "pages/tabbar/API/index"
  ],

  "preloadRule": {
    "pages/tabbar/component/index": {
      "packages": ["pages/component"]
    },
    "pages/tabbar/extended/index": {
      "packages": ["pages/extended"],
      "network": "all"
    },
    "pages/tabbar/API/index": {
      "packages": ["packageAPI"]
    }
  },
  "tabBar": {
    "custom": false,
    "color": "@tabFontColor",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "@tabBgColor",
    "borderStyle": "@tabBorderStyle",
    "position": "bottom",
    "list": [
      {
        "iconPath": "image/icon_component.png",
        "selectedIconPath": "image/icon_component_HL.png",
        "pagePath": "pages/tabbar/component/index",
        "text": "Component"
      },
      {
        "iconPath": "image/icon_component.png",
        "selectedIconPath": "image/icon_component_HL.png",
        "pagePath": "pages/tabbar/extended/index",
        "text": "Extended"
      },
      {
        "iconPath": "image/icon_API.png",
        "selectedIconPath": "image/icon_API_HL.png",
        "pagePath": "pages/tabbar/API/index",
        "text": "API"
      }
    ]
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarTitleText": "Mini Program",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle"
  },
  "subpackages": [
    {
      "root": "pages/component",
      "pages": [
        "pages/view/view",
        "pages/swiper/swiper",
        "pages/switch/switch",
        "pages/text/text",
        "pages/textarea/textarea",
        "pages/video/video",
        "pages/web-view/web-view",
        "pages/web-view-custom-nav/web-view-custom-nav",
        "pages/picker/picker",
        "pages/radio/radio",
        "pages/scroll-view/scroll-view",
        "pages/slider/slider",
        "pages/rich-text/rich-text",
        "pages/checkbox/checkbox",
        "pages/cover-view/cover-view",
        "pages/form/form",
        "pages/icon/icon",
        "pages/image/image",
        "pages/canvas/canvas",
        "pages/canvas/texImage2D/texImage2D",
        "pages/canvas/texSubImage2D/texSubImage2D",
        "pages/canvas/shader/shader",
        "pages/canvas/clear/clear",
        "pages/canvas/bufferData/bufferData",
        "pages/canvas/drawArray/drawArray",
        "pages/canvas/drawElements/drawElements",
        "pages/canvas/glsl-model/glsl-model",
        "pages/canvas/3d-rendering/3d-rendering"
      ]
    },
    {
      "root": "pages/extended",
      "name": "pages/component",
      "pages": ["pages/navigation/navigation"]
    },
    {
      "root": "pages/packageAPI",
      "name": "packageAPI",
      "pages": [
        "pages/get-system-info/get-system-info",
        "pages/make-phone-call/make-phone-call",
        "pages/navigate-in-mp/navigate-in-mp",
        "pages/animation/animation",
        "pages/animation-scroll-view/animation-scroll-view",
        "pages/application-onoff-event/application-onoff-event",
        "pages/authorize/authorize",
        "pages/get-phone-number/get-phone-number",
        "pages/get-email-address/get-email-address",
        "pages/background-fetch/background-fetch",
        "pages/navigate-to-mp-deeplink/navigate-to-mp-deeplink",
        "pages/navigation-bar/navigation-bar",
        "pages/network/network",
        "pages/page-scroll-reach-bottom/page-scroll-reach-bottom",
        "pages/preview-image/preview-image",
        "pages/pull-down-refresh/pull-down-refresh",
        "pages/read-file/read-file",
        "pages/request-payment/request-payment",
        "pages/request-subscribe-message/request-subscribe-message",
        "pages/save-image-to-photos-album/save-image-to-photos-album",
        "pages/show-toast/show-toast",
        "pages/show-action-sheet/show-action-sheet",
        "pages/set-keep-screen-on/set-keep-screen-on",
        "pages/sensor/sensor",
        "pages/scan-code/scan-code",
        "pages/canvas-to-temp-file-path/canvas-to-temp-file-path"
      ]
    },
    { "root": "pages/about", "pages": ["about"], "independent": true },
    { "root": "pages/welcome", "pages": ["welcome"], "independent": true }
  ],
  "themeLocation": "./theme.json"
}
